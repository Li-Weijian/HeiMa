<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>SendCookieServlet</servlet-name>
        <servlet-class>com.liweijian.cookie.SendCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LastAccessTimeServlet</servlet-name>
        <servlet-class>com.liweijian.cookie.LastAccessTimeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet1</servlet-name>
        <servlet-class>com.liweijian.session.SessionServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet2</servlet-name>
        <servlet-class>com.liweijian.session.SessionServlet2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.liweijian.checkimg.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckImgServlet</servlet-name>
        <servlet-class>com.liweijian.checkimg.CheckImgServlet</servlet-class>
    </servlet>
    


    <servlet-mapping>
        <servlet-name>CheckImgServlet</servlet-name>
        <url-pattern>/checkImg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionServlet2</servlet-name>
        <url-pattern>/session2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionServlet1</servlet-name>
        <url-pattern>/session1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LastAccessTimeServlet</servlet-name>
        <url-pattern>/lastAccessTime</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendCookieServlet</servlet-name>
        <url-pattern>/sendCookie</url-pattern>
    </servlet-mapping>
</web-app>